*{box-sizing:border-box}html{line-height:1.6}body{margin:0;font-family:segoe ui,Tahoma,Geneva,Verdana,sans-serif;font-weight:350;background:#121212;color:#fff}content{max-width:500px;font-weight:350;margin:auto}h1,h2,h3,h4,h5,h6{color:#fff}a{color:pink;transition:color .35s;text-decoration:none}a:hover{color:#8a2be2}::selection{background:rgba(175,186,196,.25)}::-moz-selection{background:rgba(175,186,196,.25)}.svgicon{fill:#fff}:hover{fill:#8a2be2}.inline-svg{display:inline-block;height:1.15rem;width:1.15rem;top:.15rem;position:relative;fill:#fff}.terminalStyles{margin-right:-5px;margin-bottom:-5px}.terminalStyles line{animation:terminal .5s ease-in-out infinite;animation-direction:alternate}@keyframes terminal{0%{opacity:1}100%{opacity:0}}.blinking-cursor{font-weight:100;font-size:30px;color:#fff;animation:.5s blink ease-in-out infinite;animation-direction:alternate}@keyframes blink{0%{opacity:1}100%{opacity:0}}.app-header{padding:2.5em;background:#121212;text-align:center}.app-header-avatar{width:15rem;height:15rem;border-radius:100%;border:.5rem solid #fff}.app-container{padding:2.5rem}.app-header-social{font-size:2em;color:#fff}.app-header-social a{margin:0 .1em}@media(min-width:940px){.app-header{position:fixed;border-right:.5px groove;top:10px;left:0;width:20rem;min-height:100vh}.app-container{max-width:65rem;margin-left:20rem}}.error-404{text-align:center}.error-404-title{text-transform:uppercase}.icon{display:inline-block;width:1em;height:1em;vertical-align:-.125em}.pagination{display:block;list-style:none;padding:0;font-size:1.5em;text-align:center;margin:3em 0}.page-item{display:inline-block}.page-item .page-link{display:block;padding:.285em .8em}.page-item.active .page-link{color:#121212;border-radius:2em;background:#fff}.post-title{color:#fff;font-size:3em;font-family:franklin gothic medium,arial narrow,Arial,sans-serif;line-height:1em}.post-content{font-family:Arial,Helvetica,sans-serif;font-size:1.3em}.post-content ::selection{background:#8a2be2}.post-content>pre,.post-content .highlight{margin:1em 0;max-width:960px}.post-content>pre,.post-content .highlight>pre,.post-content .highlight>div{border-left:.4em solid rgba(255,255,255,.8);padding:.5em 1em}.post-content div.highlight{max-width:960px}.post-content div.highlight pre{white-space:pre-wrap;word-break:break-word}.post-content img{max-width:100%}.post-meta{font-size:1em}.posts-list{padding:0}.posts-list-item{list-style:none;padding:.4em 0;font-size:1.2em;padding-bottom:2em}.posts-list-item:not(:last-child){border-bottom:6px rgba(255,255,255,.3)}.posts-list-item-description{display:block;font-size:1em;font-family:segoe ui,Tahoma,Geneva,Verdana,sans-serif}.posts-list-item-title{display:block;font-size:1.6em;font-family:franklin gothic medium,arial narrow,Arial,sans-serif}.tag{display:inline-block;margin-right:.2em;padding:0 .6em;font-size:.9em;border-radius:.2em;white-space:nowrap;background:rgba(255,255,255,.1);transition:color .35s,background .35s}.tag:hover{transition:color .25s,background .05s;background:rgba(255,255,255,.3)}.tags-list{padding:1}.tags-list-item{list-style:none;padding:.4em 0}.tags-list-item:not(:last-child){border-bottom:1px dashed rgba(255,255,255,.3)}@media(min-width:450px){.tags-list{display:flex;flex-wrap:wrap}.tags-list-item{width:calc(50% - 1em)}.tags-list-item:nth-child(even){margin-left:1em}.tags-list-item:nth-last-child(2){border:none}}